{
    "version": "1.7.3",
    "compilerOptions": {
        "outDir": "build/es5",
        "target": "es5",
        "module": "commonjs",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "stripInternal": true
    },
    "exclude": [
        "built",
        "node_modules"
    ]
}
